    "terms":  [
                  {
                      "term_text":  "end-user device",
                      "term_id":  "end-user-device",
                      "anchor":  "#end-user-device",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "fd38fa11aebfff9e04c3d42877682e5978c0f53b23fd826f46b52ab2e29f78ad",
                      "definition_text_excerpt":  "A phone, tablet, laptop, desktop, or hardware authenticator operated by a natural person in signature workflows."
                  },
                  {
                      "term_text":  "signature creation data",
                      "term_id":  "signature-creation-data",
                      "anchor":  "#signature-creation-data",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "1d99ae4cea89b9af8c0bca11914e46d22c12d4cf8bd5cf9543cfc33ff6147be0",
                      "definition_text_excerpt":  "Data uniquely used by the signatory to create an electronic signature; aligned with eIDAS terminology for electronic signature creation data ( [EU-910/2014] )."
                  },
                  {
                      "term_text":  "signature creation key",
                      "term_id":  "signature-creation-key",
                      "anchor":  "#signature-creation-key",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "42954c2d49a39548b71321a1c79dabbeaa195aa6808ad498f39a94366f831061",
                      "definition_text_excerpt":  "Cryptographic private key material used as [=signature creation data=] in digital signature mechanisms ( [RFC7515] , [RFC9052] )."
                  },
                  {
                      "term_text":  "hardware-backed key",
                      "term_id":  "hardware-backed-key",
                      "anchor":  "#hardware-backed-key",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "f814c7051b5c4d6588899e498c6c27a9424f38e4e06c69adb254efa24b996af3",
                      "definition_text_excerpt":  "A [=signature creation key=] whose generation, storage, and cryptographic use are enforced by protected hardware boundaries with non-exportability controls ( [ANDROID-KEYSTORE] , [MSFT-TPM] , [APPLE-SE] )."
                  },
                  {
                      "term_text":  "qualified signature creation device",
                      "term_id":  "qualified-signature-creation-device",
                      "anchor":  "#qualified-signature-creation-device",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "57f2fcd1ac222b58fc1f89c95825448369d7a7b086665715bb38a14b9959b15a",
                      "definition_text_excerpt":  "Device class defined by eIDAS and evaluated against Annex II outcomes and referenced assessment standards ( [EU-910/2014] , [EU-2016/650] )."
                  },
                  {
                      "term_text":  "sole control",
                      "term_id":  "sole-control",
                      "anchor":  "#sole-control",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "6e0a6cf84ef02297d3155095ab38cb64ed2d2f71a5fc78415921b02ae6c3b226",
                      "definition_text_excerpt":  "Security objective that signature creation data can be used only under control of the legitimate signatory, as required by Annex II outcome language ( [EU-910/2014] )."
                  },
                  {
                      "term_text":  "user intent ceremony",
                      "term_id":  "user-intent-ceremony",
                      "anchor":  "#user-intent-ceremony",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "b3ef2d2bb83f0daaf587ff71a5e7797008e28e8f3ec9c9308149d8334b8bd861",
                      "definition_text_excerpt":  "Complete interaction in which the signer can inspect what is being signed and explicitly authorize the operation, with security-critical portions mediated by trusted platform paths when available ( [ANDROID-PROTECTED-CONFIRMATION] , [WEBAUTHN-L3] )."
                  },
                  {
                      "term_text":  "user approved object",
                      "term_id":  "user-approved-object",
                      "anchor":  "#user-approved-object",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "233303459ba1247acff0b257f7ea6bcd585624a713be1d6195b82844be9f2183",
                      "definition_text_excerpt":  "The exact full payload byte sequence presented to and approved by the signer before signature creation; this profile does not permit signing operations where only a partial or transformed subset is shown as the approval target ( [EU-ANNEX-II] , [ANDROID-PROTECTED-CONFIRMATION] , [RFC7515] , [RFC9052"
                  },
                  {
                      "term_text":  "proof object",
                      "term_id":  "proof-object",
                      "anchor":  "#proof-object",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "a1a7c210e96f492cc8aae36924c5f38184780e6fd458b6d583194ddc3875d8e0",
                      "definition_text_excerpt":  "JSON object attached to an artifact that carries cryptographic proof metadata and proof bytes so a verifier can check integrity and authenticity deterministically ( [VC-DI] )."
                  },
                  {
                      "term_text":  "DataIntegrityProof",
                      "term_id":  "dataintegrityproof",
                      "anchor":  "#dataintegrityproof",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "21e0f4857721363bbb4329339bae1858737b45863b7e04031da86f2c05558ab4",
                      "definition_text_excerpt":  "[=proof object=] type defined by the W3C Data Integrity model; in this profile it is used for profile-level integrity claims and is paired with the selected cryptosuite and proofValue members ( [VC-DI] , [VC-DI-ECDSA] )."
                  },
                  {
                      "term_text":  "zero-trust verification model",
                      "term_id":  "zero-trust-verification-model",
                      "anchor":  "#zero-trust-verification-model",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "1ea5cc7c67dc863e5f4b31c3a0030e32b7a303d30980fe9d39742d43e834b093",
                      "definition_text_excerpt":  "Verification model where each decision is based on explicit per-transaction evidence and policy evaluation, rather than implicit trust in network location, client software, or governance labels alone ( [NIST-SP-800-207] , [RFC7515] , [RFC9052] )."
                  },
                  {
                      "term_text":  "trusted computing base",
                      "term_id":  "trusted-computing-base",
                      "anchor":  "#trusted-computing-base",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "a1ccc962669a0172ddac2d542f046286d70abf7c676fc0f5188bf640dc96190e",
                      "definition_text_excerpt":  "Set of hardware, firmware, and software components that must behave correctly to uphold stated security properties ( [ETSI-319401] )."
                  },
                  {
                      "term_text":  "evidence record",
                      "term_id":  "evidence-record",
                      "anchor":  "#evidence-record",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "224f75ac210a4cbf4872b8a44b103bab4c296a48f291e1e35afb2d16fa8de3b0",
                      "definition_text_excerpt":  "Machine-verifiable artifact set used to substantiate technical or legal property claims ( [ETSI-319401] , [EU-2016/650] )."
                  },
                  {
                      "term_text":  "gqtsp discovery endpoint",
                      "term_id":  "gqtsp-discovery-endpoint",
                      "anchor":  "#gqtsp-discovery-endpoint",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "8e2966cbbd5e56ccfd3120fb01caf5c3ff4d0f1d0816546608b4b15e43724325",
                      "definition_text_excerpt":  "Deployment-defined HTTPS discovery location used to publish evidence metadata and pointers for decentralized verifier retrieval in this profile ( [RFC8615] , [RFC9110] , [GDIS-CORE] , REQ-GDIS-05 , getSchemeDescriptor , getEndpointKindCatalog , getEventLogView , and getEventHeadMeta )."
                  },
                  {
                      "term_text":  "local signing profile",
                      "term_id":  "local-signing-profile",
                      "anchor":  "#local-signing-profile",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "edce6fc309d95aa0c282dd42316716bc1a4c7a5422a350c3d93557ac5e1ce60c",
                      "definition_text_excerpt":  "Model where signature creation data remains on the signatory-operated [=end-user device=] and is not duplicated to remote service custody."
                  },
                  {
                      "term_text":  "remote signing model",
                      "term_id":  "remote-signing-model",
                      "anchor":  "#remote-signing-model",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "9e8502b0ac3a8d6096c3e34b10deb74cc82295f2a581c2d1a2e8119657b408da",
                      "definition_text_excerpt":  "Model where signature creation data is created, managed, or activated in server-side trust-service components external to the signatory device ( [CEN-419241-1] , [ETSI-119431-1] , [ETSI-119431-2] )."
                  },
                  {
                      "term_text":  "web profile",
                      "term_id":  "term-web-profile",
                      "anchor":  "#term-web-profile",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "6f1f5d835b0102673af483547c629ef062740cd368b7bea8d0e4173b57de832f",
                      "definition_text_excerpt":  "Primary interoperability profile in this specification, based on globally deployed web and internet standards ( [WEBCRYPTO-L2] , [WEBAUTHN-L3] , [RFC7515] , [RFC9052] )."
                  },
                  {
                      "term_text":  "eu compatibility profile",
                      "term_id":  "term-eu-compatibility-profile",
                      "anchor":  "#term-eu-compatibility-profile",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "5767712e793c12845c80936994ff52b440f12c934e9fcf525ea3a986f2ecb161",
                      "definition_text_excerpt":  "Mapping layer from [=web profile=] artifacts to EU legal and ecosystem evidence requirements ( [EU-910/2014] , [EU-2016/650] , [EU-2024/2979] , [EU-2024/2982] )."
                  },
                  {
                      "term_text":  "qualified status claim",
                      "term_id":  "qualified-status-claim",
                      "anchor":  "#qualified-status-claim",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "3c94fde09845d9e92b59440859b9516dd5c357ef10ec723cadb7d763ff7ed945",
                      "definition_text_excerpt":  "Claim that an actor, service, device, or signature has legal qualified status within a defined jurisdiction; such claims require authoritative status evidence ( [EU-TRUSTED-LISTS] , [EU-QSCD-NOTIFICATIONS] )."
                  },
                  {
                      "term_text":  "eidas framework",
                      "term_id":  "eidas-framework",
                      "anchor":  "#eidas-framework",
                      "section_anchor":  "#conformance",
                      "definition_excerpt_hash":  "2d7e83dc5bb2992015290d32bb5f330e9a00496d4f4f064d88a642c14909bbca",
                      "definition_text_excerpt":  "EU legal framework for electronic identification and trust services established by Regulation (EU) No 910/2014 and its amendments ( [EU-910/2014] , [EU-2024/1183] )."
                  },
                  {
                      "term_text":  "qscd certification evidence tuple",
                      "term_id":  "qscd-certification-evidence-tuple",
                      "anchor":  "#qscd-certification-evidence-tuple",
                      "section_anchor":  "#eu-certification-notification-path",
                      "definition_excerpt_hash":  "92140e28462a4459f56c9ad0e034aa912ca26ec102f61bcc1134c7c2aeff4c56",
                      "definition_text_excerpt":  "{ certificate of conformity, certification report, certification report URL, certified configuration identifiers (hardware/firmware/software versions), validity dates and status, certification body identity }"
                  }
              ],
